{ open LogBlareYacc}rule token = parse   [' ' '\t']     { token lexbuf }     (* mange les blancs et les tab  *)  | ['\n' ]        { EOL }  | eof            { EOF}        |[';'] {PTVIRG}  | ['<'] {CH_OUV}  | ['>'] {CH_FER}  | ['{'] {ACC_OUV}  | ['}'] {ACC_FER}  | ['['] {CR_OUV}  | [']'] {CR_FER}  | [':'] {DXPTS}  |  (['0'-'9']*) as lxm  { INT ( lxm)}  |  (['0'-'9' '.']*) as lxm  { FLOAT ( lxm)}  |  ( ['a'-'z']* | ['A'-'Z']* | ['/''_''#' '.' '-' ]*|['0'-'9']*)* as lxm  { STRING (lxm)} 