{ open Log_yacc}  rule token = parse   [' ' '\t']     { token lexbuf }     (* mange les blancs et les tab  *)  | ['\n' ]        { EOL }  | eof            { EOF}        |[';'] {PTVIRG}  | ['<'] {FLECHE}  | ['-'] {TIRET}  | ['c'] {CALL}  | ['r'] {RESPONSE}   |  (['0'-'9']*) as lxm  { INT ( lxm)}    |  ( ['a'-'z']* | ['A'-'Z']* | ['/''_' ]*|['0'-'9']*)* as lxm  { STRING (lxm)} 